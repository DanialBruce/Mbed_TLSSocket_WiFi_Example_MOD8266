{   
    "config": {
        "use-tls-socket": {
            "value" : true
        },
        "wifi-ssid": {
            "help": "WiFi SSID",
            "value": "\"CHANGE_SSID_HERE\""
        },
        "wifi-password": {
            "help": "WiFi Password",
            "value": "\"CHANGE_WIFI_PASS_HERE\""
        },
        "esp-rx-pin": {
            "help": "ESP8266 module RX pin",
            "value": "D0"
        },
        "esp-tx-pin": {
            "help": "ESP8266 module TX pin",
            "value": "D1"
        },
        "mqtt-id": {
            "help": "MQTT Client id",
            "value": "\"MUCH_UNIQUE_ID\""
        },
        "mqtt-broker-ip": {
            "help": "MQTT Broker host ip.",
            "value": "\"CHANGE_IF_USE_BROKER_IP\""
        },
      "mqtt-broker-hostname": {
            "help": "MQTT Broker resolvable hostname.",
            "value": "\"CHANGE_BROKER_DNS_ADDRESS_HERE\""
        },
        "mqtt-broker-port": {
            "help": "MQTT Broker port",
            "value": "8886"
        },
        "mqtt-topic": {
            "help": "MQTT topic to publish",
            "value": "\"CHANGE_TOPIC_HERE\""
        }
    },
    "target_overrides": {
            "*": {
            "platform.stdio-baud-rate": 9600,
            "rtos.main-thread-stack-size": 8192,
            "mbed-mqtt.max-connections": "5",
            "mbed-mqtt.max-packet-size": "1024",
            "esp8266.provide-default" : false
            }
        }
}